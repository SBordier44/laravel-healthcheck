{
  "name": "nuboxdevcom/laravel-healthcheck",
  "description": "Liveness and Readiness Probes in your Laravel application",
  "type": "library",
  "license": "MIT",
  "version": "1.0.1",
  "authors": [
    {
      "name": "NuBOX DevCom",
      "email": "slye@nubox.fr",
      "role": "Developer"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "ext-pdo": "*",
    "illuminate/support": "^9.25"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "NBXDC\\LaravelHealthCheck\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "NBXDC\\LaravelHealthCheck\\HealthCheckServiceProvider"
      ]
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
